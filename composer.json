{
    "name": "bentools/doctrine-ulid",
    "description": "ULID (Universally Unique Lexicographically Sortable Identifier) support for Doctrine IDs",
    "keywords": [
        "ulid",
        "uuid",
        "doctrine",
        "orm"
    ],
    "authors": [
      {
        "name": "Beno!t POLASZEK"
      },
      {
          "name": "Maximilian Schieder",
          "email": "maximilian.schieder@posteo.net"
      }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "robinvdvleuten/ulid": "^5.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.8",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "@stable",
        "php-coveralls/php-coveralls": "@stable",
        "matthiasnoback/doctrine-orm-test-service-provider": "^3.0",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "classmap": [
            "src/ULIDGenerator.php",
            "src/GeneratedULIDTrait.php",
            "src/EditableULIDTrait.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BenTools\\ULID\\Tests\\": "tests"
        }
    }
}
